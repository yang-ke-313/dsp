<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.UnderlyingCategoryMapper">

    <resultMap id="underlyingcategory" type="com.entity.UnderlyingCategory">
        <id property="id" column="id"></id>
        <result property="title" column="title"></result>
        <result property="cate1_id" column="cate1_id"></result>
        <result property="level" column="level"></result>
        <result property="time_create" column="time_create"></result>
        <result property="time_create" column="time_create"></result>
    </resultMap>

    <select id="findByCate1_id" resultMap="underlyingcategory">
    select * from category_2 where cate1_id = #{cate1_id}
    </select>

    <select id="getAll" resultMap="underlyingcategory">
    select * from category_2
    </select>

</mapper>